{
  "name": "reduction",
  "private": true,
  "description": "A starter for a blog powered by Gatsby and Markdown",
  "version": "0.1.0",
  "author": "Eric Bower <e@erock.io>",
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "dependencies": {
    "@babel/core": "7.11.6",
    "@emotion/core": "10.0.5",
    "@mdx-js/react": "1.0.27",
